pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                // Exemplo de comando de build
                sh 'echo "Building..."'
            }
        }
        
        stage('Test') {
            steps {
                // Exemplo de comando de teste
                sh 'echo "Running Tests..."'
            }
        }
        
        stage('Deploy') {
            steps {
                // Exemplo de comando de deploy
                sh 'echo "Deploying..."'
            }
        }
    }
}



