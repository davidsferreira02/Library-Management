pipeline {
    agent any

    stages {

        stage('Build') {
            steps {
                sh 'echo "Building..."'
                sh 'mvn clean install'
            }
        }

        stage('Package') {
            steps {
                sh 'echo "Packaging..."'
                sh 'mvn package'
            }
        }

        stage('Unit Tests') {
            steps {
                sh 'echo "Running Unit Tests..."'
                sh 'mvn test'
            }
        }

    }
}



